{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "arrow-spacing": "warn",
        "valid-typeof": "error",
        "strict": "warn",
        "curly": "warn",
        "indent": "warn",
        "no-empty-function": "off"
    }
}
